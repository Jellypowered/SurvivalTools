{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "BuildWindowsDLL: Debug",
            "type": "shell",
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "command": ".vscode/build.bat",
            "args": [
                "${input:buildConfig}"
            ]
        }
    ],
    "inputs": [
        {
            "id": "buildConfig",
            "type": "pickString",
            "description": "Select build configuration",
            "options": [
                "Debug",
                "Release"
            ],
            "default": "Debug"
        }
    ]
}
